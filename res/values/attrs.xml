<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GridItem1">
        <attr name="gridImage" format="reference" />
        <attr name="toggleImage" format="reference" />
        <attr name="checked" format="boolean" />
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:paddingLeft" />
        <attr name="android:paddingBottom" />
    </declare-styleable>
    <declare-styleable name="GridItem2">
        <attr name="gridImage" />
        <attr name="title" />
        <attr name="titleColor" format="reference|color" />
        <attr name="titleSize" format="reference|dimension" />
        <attr name="subDes" format="reference|string" />
        <attr name="subDesColor" format="reference|color" />
        <attr name="subDesSize" format="reference|dimension" />
        <attr name="withPop" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="barLength" format="dimension" />
        <attr name="contourColor" format="color" />
        <attr name="contourSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="BatteryProgress">
        <attr name="progressColor" format="color" />
        <attr name="batteryContainer" format="reference" />
    </declare-styleable>

</resources>